{
  "name": "ecommerce",
  "version": "1.0.0",
  "description": "MERN Ecommerce",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && cross-env NODE_ENV=production webpack --mode production --config webpack/webpack.prod.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config webpack/webpack.dev.js",
    "dev:server": "nodemon --exec babel-node server.js",
    "dev": "npm-run-all --parallel dev:*",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "heroku-postbuild": "npm run build",
    "postinstall": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "babel": "babel src -d dist --config-file ../babel.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mohamedsamara/mern-ecommerce.git"
  },
  "keywords": [
    "node",
    "express",
    "mongoose",
    "react",
    "redux",
    "redux-thunk",
    "webpack"
  ],
  "author": "Ewaru Emmanuel (https://github.com/ewaru-emmanuel)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ewaru-emmanuel/ecommerce/"
  },
  "homepage": "https://github.com/ewaru-emmanuel/ecommerce#readme",
